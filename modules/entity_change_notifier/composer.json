{
    "name": "drupal/entity_change_notifier",
    "type": "drupal-module",
    "description": "Notify about entity updates",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/jsonapi": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.7",
        "drupal/coder": "^2.0|^8.2",
        "drupal/drupal-extension": "master-dev",
        "drush/drush": "~8.1|~9.0",
        "behat/behat": "3.3.1"
    },
    "suggest": {
        "drupal/rabbitmq": "Allows using RabbitMQ as a queue destination"
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\entity_change_notifier\\": "tests/src"
        }
    }
}
